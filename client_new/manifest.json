{
    "manifest_version": 3,
    "name": "FoodaPlus",
    "description": "Adds Reviews to fooda!",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["injection.js"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": [
        "*"
    ]
}